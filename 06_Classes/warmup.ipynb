   "source": [
    "# Create a function blend_fruits that takes to fruits as input and returns a mix of them as output\n",
    "# >>> mix_fruits(\"apple\", \"banana\")\n",
    "# \"applebanana\"\n",
    "\n",
    "\n",
    "def blend_fruits(a, b):\n",
    "    return f\"{a}{b}\"\n",
    "\n",
    "\n",
    "print(blend_fruits(\"apple\", \"banana\"))"
   ]
  }
